{%- macro infobox() -%}
	{{- "{{infobox" -}}
	{{- '\n' -}}
	{{- '| title = {}'.format(book_title) -}}
	{{- '\n' -}}
	{%- if authors -%}
	    {{- '| header2 = ' -}}
	    {{- '\n' -}}
	    {{- "| label2 = {}".format('Author') -}}
	    {{- '\n' -}}
	    {{- "| data2 = {}".format(authors[0]) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if book_title -%}
	    {{- '| header3 = ' -}}
	    {{- '\n' -}}
	    {{- "| label3 = {}".format('Original title') -}}
	    {{- '\n' -}}
	    {{- "| data3 = {}".format(book_title) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if subtitle -%}
	    {{- '| header4 = ' -}}
	    {{- '\n' -}}
	    {{- "| label4 = {}".format('Subtitle') -}}
	    {{- '\n' -}}
	    {{- "| data4 = {}".format(subtitle) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if language -%}
	    {{- '| header5 = ' -}}
	    {{- '\n' -}}
	    {{- "| label5 = {}".format('Language') -}}
	    {{- '\n' -}}
	    {{- "| data5 = {}".format(language) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if categories -%}
	    {{- '| header6 = ' -}}
	    {{- '\n' -}}
	    {{- "| label6 = {}".format('Genre') -}}
	    {{- '\n' -}}
	    {{- "| data6 = {}".format(categories) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if publisher -%}
	    {{- '| header7 = ' -}}
	    {{- '\n' -}}
	    {{- "| label7 = {}".format('Publisher') -}}
	    {{- '\n' -}}
	    {{- "| data7 = {}".format(publisher) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if published_date -%}
	    {{- '| header8 = ' -}}
	    {{- '\n' -}}
	    {{- "| label8 = {}".format('Publication date') -}}
	    {{- '\n' -}}
	    {{- "| data8 = {}".format(published_date) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if page_count -%}
	    {{- '| header9 = ' -}}
	    {{- '\n' -}}
	    {{- "| label9 = {}".format('Pages') -}}
	    {{- '\n' -}}
	    {{- "| data9 = {}{}".format(page_count,REF_GG) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if Dimensions -%}
	    {{- '| header10 = ' -}}
	    {{- '\n' -}}
	    {{- "| label10 = {}".format('Dimensions') -}}
	    {{- '\n' -}}
	    {{- "| data10 = {}{}".format(Dimensions,REF_AM) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if isbn_13 -%}
	    {{- '| header11 = ' -}}
	    {{- '\n' -}}
	    {{- "| label11 = {}".format('ISBN-13') -}}
	    {{- '\n' -}}
	    {{- "| data11 = {}".format(isbn_13) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if isbn_10 -%}
	    {{- '| header12 = ' -}}
	    {{- '\n' -}}
	    {{- "| label12 = {}".format('ISBN-10') -}}
	    {{- '\n' -}}
	    {{- "| data12 = {}".format(isbn_10) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{%- if worldcat_oclc_number -%}
	    {{- '| header13 = ' -}}
	    {{- '\n' -}}
	    {{- "| label13 = {}".format('OCLC') -}}
	    {{- '\n' -}}
	    {{- "| data13 = {}".format(worldcat_oclc_number) -}}
	    {{- '\n' -}}
	{%- endif -%}
	
	{{- '}}' -}}
	{{- '\n' }}
{%- endmacro -%}


{%- macro intro(book_title, subtitle, published_date, authors, publisher,language, categories, description, maturity_rating) -%}
	
    {{ "\n==పరిచయం==\n"}}
            
        {%- set line1A = [book_title,' అను పుస్తకం ']|join('') -%}
        {%- set line1B = [book_title,' అనే పేరుగల పుస్తకం ']|join('') -%}
        
        {%- if published_date|length==4-%}
            {%- set line2A =  [published_date,' సంవత్సరం లో విడుదల అయింది. ']|join('') -%}
        {%- else -%}
            {%- set line2A =  [published_date,' నాడు విడుదల అయింది. ']|join('') -%}
        {%- endif -%}
        

        {%- set authors_string = authors|join(', ') -%}

            {%-if published_date==''-%}
                {%- set line3A = [authors_string,' చే రచింపబడింది. ']|join('') -%}
                {%- set line3B = [authors_string,' చే రచింపబడింది. ']|join('') -%}
            {%-else-%}
                {%- set line3A = [' దీనిని ',authors_string,' రచించారు. ']|join('') -%}
                {%- set line3B = [' ఈ పుస్తకం ',authors_string,' చే రచింపబడింది. ']|join('') -%}
            {%-endif-%}

        {%- set line4A = [publisher,' అనే సంస్థ ఈ పుస్తకాన్ని ప్రచురించింది. ']|join('') -%}
        {%- set line4B = [publisher,' అనే సంస్థ ఈ పుస్తకాన్ని ప్రచురించే కర్తవ్యాన్ని నిర్వర్తించింది. ']|join('') -%}

        {%- set line5A = ['ఈ పుస్తకం ', language ,' భాషలో ప్రచురింపబడినది. ']|join('') -%}
        {%- if authors|length>1 -%}
            {%- set line5B = ['రచయితలు ఈ పుస్తకాన్ని ', language ,' భాషలో రచించారు. ']|join('') -%}
        {%- else -%}
            {%- set line5B = ['ఈ పుస్తకం ', language ,' భాషలో ప్రచురింపబడినది. ']|join('') -%}
        {%-endif-%}

        {%- set line6A = [categories, ' రచనాశైలి లో ఈ పుస్తకం వ్రాయబడినది. ']|join('') -%}
        {%- set line6B = [categories, ' రచనాశైలి లోని అనేక అంశాలు ఈ పుస్తకం లో కొట్టొచ్చినట్లు కనిపిస్తాయి. ']|join('') -%}

        {%- set line7A  = ['ఈ పుస్తకానికి ' ,maturity_rating,' రేటింగ్ ఇవ్వబడినది. ',REF_GG]|join('')-%}
        {%- set line7B  = ['సాంఘిక ప్రమాణాలకు అనుగుణంగా ఈ పుస్తకానికి ',maturity_rating,' వ రేటింగ్ కేటాయించబడినది. ',REF_GG]|join('') -%}
        
        {{-[ line1A, line1B ]|random-}}

        {%- if subtitle!=''-%}
            {%- set auxLine1 = [' (ఉప శీర్షిక: ' ,subtitle,' ) ']|join('')-%}
            {{-auxLine1-}}
        {%- endif -%}

        {{-line2A-}}
        {{-[ line3A, line3B ]|random-}}
        {{-[ line4A, line4B ]|random-}}
        {{-[ line5A, line5B ]|random-}}
        {{-[ line6A, line6B ]|random-}}

        {%- if description!=''-%}
            {%- set auxLine2 = [description,' అనేది ఈ పుస్తక నేపథ్యమును సంగ్రహ పరుస్తుంది. ']|join('')-%}
            {{-auxLine2-}}
        {%- endif -%}

        {{-[ line7A, line7B ]|random-}}

{%- endmacro -%}


{%- macro summary( librarything_characters, librarything_book_summary,author1_image) -%}
	
    {%- if librarything_characters=='' and librarything_book_summary =='' -%}
        {%- if author1_image!=''-%}
            {{-['[[',author1_image,'|thumb|187x187px|రచయిత]]']|join('') -}}
        {%- endif -%}
    
    {%- else -%}
    
        
        {{"\n==పుస్తకంలోని విషయాల వివరాలు==\n"}}
                
            
            {%- if librarything_characters != '' -%}
                {{"\n===పాత్రలు===\n"}}
                {%- set lenlis = librarything_characters|length -%}
                {{-['ఈ పుస్తకం లో కనిపించే పాత్రలు',REF_LT]|join('')-}}
                {%-for i in range(0,lenlis)-%}
                    {{-"\n"-}}
                    {{['* ',librarything_characters[i]]|join('')}}
                {%- endfor -%}
            {%- endif -%}
                
            {%- if librarything_book_summary != '' -%}
                {{-"\n"-}}
                {{"\n===కథానిక===\n"}}
                {{- ['ఈ పుస్తకం కథానిక','- ',librarything_book_summary,REF_LT]|join('') -}}
            {%- endif -%}
            
            {{-"\n"-}}
            {%- if author1_image!=''-%}
                {{-['[[File:',author1_image,'|thumb|187x187px|రచయిత]]']|join('') -}}
            {%- endif -%}

    {%-endif-%}
        

{%- endmacro -%}



{%- macro awardsReceived(goodreads_awards) -%}
	
    {{ "\n==పురస్కారాలు==\n"}}
        
        {%- if goodreads_awards != '' -%}
            
            {%- set line1A = ['ఈ పుస్తకం ఎన్నో పురస్కారాలను గెలుచుకుంది. ']|join('') -%}
            {%- set line1B = ['ఈ పుస్తకం ఎన్నో అవార్డులతో సన్మానించబడింది. ']|join('') -%}

            {%- set awardslen = goodreads_awards|length-%}

            {%- set line3A  = ' వాటిలో కొన్ని ప్రాముఖ్యత చెందిన పురస్కారాలు',REF_GR,'- '-%}
            {%- set line3B  = ' వాటన్నింటిలో కొన్ని ముఖ్యమైనవి',REF_GR,'-  ' -%}
            
            {{-[ line1A, line1B ]|random-}}

            {%-for i in range(0,awardslen)-%}
                    {{-"\n"-}}
                    {{['* ',goodreads_awards[i]]|join('')}}
                {%- endfor-%}


            {{-[ line3A, line3B ]|random-}}

        {%- else -%}

           {# {{'ఈ పుస్తకం చేజిక్కించుకున్న అవార్డుల జాబితాకు సంబంధించిన డేటా అందుబాటులో లేకపోవడం వలన వాటిని ఈ వ్యాసంలో విశ్లేషించలేకపోయాము.'}} #}
           {# removed this statement because we've been told to keep sections empty if no data is available #}
        {%- endif -%}

{%- endmacro -%}



{%- macro ratingsReceived(bestseller_rank, amazon_ratings_count, amazon_rating, ratings_count, average_rating, goodreads_ratings_count, goodreads_rating, librarything_reviews, librarything_avg_rating) -%}

    {{ "\n==రేటింగ్స్==\n"}}

        {{-'ఈ పుస్తకాన్ని ఎంతో మంది చదివి ఆదరించారు.'-}}
        
        {%- if bestseller_rank != '' -%}
            
            {%- set line2A = [' ఈ పుస్తకాన్ని అమెజాన్ అనే వెబ్‌సైట్ కొనుగోళ్ల ఆధారంగా ',bestseller_rank, ' అనే ర్యాంక్ ని కేటాయించింది. '] |join('') -%}
            {%- set line2B = [' అమెజాన్ అనే వెబ్ సైట్ లో ఈ పుస్తకానికి జరిగిన విక్రయాల ఆధారంగా దీనికి ',bestseller_rank, ' వ ర్యాంకును కేటాయించింది. '] |join('') -%}
            {{[line2A, line2B]|random}}

        {%- endif -%}
        
        {{"\n"}}

        {%- set opener_3A = ''-%}
        {%- set infodict = ({'anyExists':0,'amazonExists':0,'goodreadsExists':0,'librarythingExists':0,'googlebooksExists':0}) -%}

        {%- if amazon_ratings_count!='' and amazon_rating!='' -%}
            {%- set _dummy = infodict.update({'amazonExists':1}) -%}
            {%- set _dummy = infodict.update({'anyExists':1}) -%}
        {%- endif -%}
        
        {%- if goodreads_ratings_count!='' and goodreads_rating!='' -%}
            {%- set _dummy = infodict.update({'goodreadsExists':1}) -%}
            {%- set _dummy = infodict.update({'anyExists':1}) -%}
        {%- endif -%}

        {%- if librarything_reviews!='' and librarything_avg_rating!='' -%}
