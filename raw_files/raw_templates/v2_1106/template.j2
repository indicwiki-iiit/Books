                {%- set _dummy = lineslis.append(temp) -%}
            {%- endif -%}

          

            {%- set lenlis = lis|length -%}

            {%- if lenlis == 1 -%}

                {%- set opener_3A = linelis[0]-%}
                {{opener_3A}}

            {%- else -%}
                {%- if lenlis==2 -%}
                    {%- set temp = [lis[0], ', ', lis[1],' అనే వెబ్సైట్లలో ఈ పుస్తకానికి లభించిన ఆదరణాభిమానాల గణాంకాలు క్రింద వివరించబడినవి. '] |join('')-%}
